{
    "training": {
        "epochs": 100000,
        "grad_accum": 8,
        "grad_clip": 1.0,
        "loss": "ce"
    },
    "model": {
        "weight_name": "linear",
        "name":"fmix"
    },
    "checkpoint": {
        "checkpoint_path": "./checkpoints-fashion/",
        "n_checkpoint": 5
    },
    "diffusion": {
        "dropout": 0.4,
        "diffusion_num_steps": 1000,
        "diffusion_num_steps_eval": 10,
        "nch": 128,
        "out_ch": 1,
        "ch_mult": [1, 1, 2, 2, 4, 4],
        "dim_in": 10,
        "dim_lat_out": 512,
        "dim_output": 10,
        "scale":{
            "ch": 128,
            "in_dim": 10,
            "out_dim": 512
        }

    },
    "overfitting": {
        "lr_overfitting": 5e-4,
        "n_overfitting": 3
    }
}

